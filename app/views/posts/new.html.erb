<%= form_for @post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :skills %>
  <%= f.text_field :skills %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :start_date %>
  <%= f.date_field :start_date %>
  <%= f.label :end_date %>
  <%= f.date_field :end_date %>
  <%= f.submit %>
  <% end %>
